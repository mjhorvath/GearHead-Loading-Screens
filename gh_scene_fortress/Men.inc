#macro Men_CSG( C1, C2, C3 )
   #local V1 = vnormalize( vcross( C2 - C1, C3 - C2 ) );
   #local Dis = abs( vdot( C1, V1 ) );
   #local V2 = vnormalize( -vcross( V1, C2 - C1 ) );
   #local V3 = vnormalize( -vcross( V1, C3 - C2 ) );
   #local V4 = vnormalize( -vcross( V1, C1 - C3 ) );
   object {
   intersection {
                  plane { -V1, ( Dis + Thickness / 2 ) }
                  plane { -V1, ( Dis - Thickness / 2 ) inverse }
                  plane { V2, vdot( V2, C1 ) }
                  plane { V3, vdot( V3, C2 ) }
                  plane { V4, vdot( V4, C3 ) }
                 }
   clipped_by { box { <min( min( C1.x, C2.x ), C3.x ), min( min( C1.y, C2.y ), C3.y ), min( min( C1.z, C2.z ), C3.z )> - Thickness, <max( max( C1.x, C2.x ), C3.x ), max( max( C1.y, C2.y ), C3.y ), max( max( C1.z, C2.z ), C3.z )> + Thickness } }
   }
#end

